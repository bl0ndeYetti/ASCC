{
  "compilerOptions": {
    "target": "ES2019",
    "module": "None",
    "lib": ["ES2019"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./apps-script",
    "outDir": "./build",
    "types": ["google-apps-script", "node"],
    "noEmit": true
  },
  "include": ["apps-script/**/*"],
  "exclude": ["node_modules", "helper/**/*", "build"]
} 